    "@testing-library/react": "^9.4.0",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
